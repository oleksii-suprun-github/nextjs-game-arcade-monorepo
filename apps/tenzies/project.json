{
  "name": "tenzies",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tenzies",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project tenzies --web",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "options": {
        "outputPath": "dist/apps/tenzies",
        "root": "apps/tenzies",
        "configurations": {
          "production": {
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "extractCss": true,
            "namedChunks": false,
            "aot": true,
            "extractLicenses": true,
            "vendorChunk": false,
            "buildOptimizer": true
          }
        }
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "tenzies:build",
        "port": 3001
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/tenzies/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
